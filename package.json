{
  "name": "webrtc",
  "productName": "webrtc",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "browserify src/rtc.js -o src/bundle.js && electron-forge start",
    "package": "browserify src/rtc.js -o src/bundle.js && electron-forge package",
    "make": "browserify src/rtc.js -o src/bundle.js && electron-forge make",
    "publish": "browserify src/rtc.js -o src/bundle.js && electron-forge publish",
    "build": "electron-packager ./src ElectronScreenRecorder  --platform=win32 --arch=x64 --app-version=0.36.0 --icon=src/Icon.icns",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Sushant Singh",
    "email": "sushantr134@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "webrtc"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "browserify": "^16.5.0",
    "domify": "^1.4.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.47",
    "electron-packager": "^14.1.1",
    "electron-prebuilt": "^1.4.13",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "electron": "7.1.7"
  }
}
